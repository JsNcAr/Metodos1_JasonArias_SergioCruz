    "def bilineal_aprox(x, y, p, N):\n",
    "    sum = 0\n",
    "    for i in range(N):\n",
    "        for j in range(N):\n",
    "            sum += p[i, j]*x**i*y**j\n",
    "    return sum\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sympy_bilineal_aprox(x, y, N):\n",
    "    sum = 0\n",
    "    x = sp.symbols('x')\n",
    "    y = sp.symbols('y')\n",
    "    for i in range(N):\n",
    "        for j in range(N):\n",
    "            a_s = sp.symbols('a'+str(i)+str(j))\n",
    "            sum += a_s*x**i*y**j\n",
    "    return sum\n"
   ]
  },
